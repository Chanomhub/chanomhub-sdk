{
  "name": "@chanomhub/sdk",
  "version": "1.0.5",
  "description": "ChanomHub SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chanomhub/chanomhub-sdk.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "exports": {
    ".": {
